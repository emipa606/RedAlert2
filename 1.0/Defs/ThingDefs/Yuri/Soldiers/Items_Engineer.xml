<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="HatBase">
    <defName>ra2_Hat_Engineer_Y</defName>
    <label>Engineer_Y hat</label>
    <description>Engineer_Y's hat.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/Tuque/Tuque</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>2200</WorkToMake>
      <Mass>0.07</Mass>
      <ArmorRating_Blunt>0.03</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.03</ArmorRating_Sharp>
      <Insulation_Cold>10</Insulation_Cold>
      <Insulation_Heat>2</Insulation_Heat>
    </statBases>
    <generateCommonality>0.5</generateCommonality>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/Tuque/Tuque</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>ra2_Hat_Engineer_Y</li>
      </tags>
      <defaultOutfitTags>
        <li>Engineer_Y</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>100</weight>
          <only>RGBA(1.0,1.0,0.00,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
